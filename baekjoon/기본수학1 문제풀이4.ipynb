    "n = int(input())\n",
    "i=1\n",
    "while True:\n",
    "    if ((i-1) * (6+(6*(i-1)))) / 2 < n-1 <= (i * (6+(6*i)))/2 :\n",
    "        break\n",
    "    else:\n",
    "        i+=1\n",
    "print(i+1)"
